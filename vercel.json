{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "functions": {
    "app/api/ai/generate/route.ts": {
      "maxDuration": 120,
      "memory": 1024
    },
    "app/api/ai/regenerate-activity/route.ts": {
      "maxDuration": 30,
      "memory": 512
    },
    "app/api/ai/generate-more-days/route.ts": {
      "maxDuration": 60,
      "memory": 512
    },
    "app/api/hotels/*/route.ts": {
      "maxDuration": 15,
      "memory": 256
    },
    "app/api/places/*/route.ts": {
      "maxDuration": 15,
      "memory": 256
    },
    "app/api/travel/*/route.ts": {
      "maxDuration": 15,
      "memory": 256
    },
    "app/api/trips/*/route.ts": {
      "maxDuration": 10,
      "memory": 256
    },
    "app/api/admin/*/route.ts": {
      "maxDuration": 30,
      "memory": 256
    }
  },
  "regions": ["iad1"]
}
